{
  "name": "@accounts/rest",
  "version": "0.0.1",
  "description": "REST client for accounts",
  "main": "lib/index.js",
  "scripts": {
    "start": "webpack -p --config --progress --watch",
    "compile": "webpack -p --config --progress",
    "prepublish": "npm run compile",
    "test": "yarn lint && yarn testonly",
    "test-ci": "yarn lint && yarn coverage",
    "testonly": "jest",
    "lint": "eslint src",
    "coverage": "yarn testonly -- --coverage",
    "coveralls": "yarn coverage && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/js-accounts/rest"
  },
  "keywords": [
    "users",
    "accounts",
    "rest"
  ],
  "author": "Tim Mikeladze",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.16.0",
    "babel-core": "^6.17.0",
    "babel-eslint": "^7.0.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-syntax-async-functions": "^6.13.0",
    "babel-plugin-transform-async-to-generator": "^6.16.0",
    "babel-plugin-transform-object-rest-spread": "^6.16.0",
    "babel-plugin-transform-regenerator": "^6.16.1",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-es2015-node4": "^2.1.0",
    "coveralls": "^2.11.14",
    "eslint": "^3.7.1",
    "eslint-config-airbnb": "^12.0.0",
    "eslint-config-airbnb-base": "^9.0.0",
    "eslint-plugin-import": "^2.0.1",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.3.0",
    "jest": "^18.0.0",
    "lodash": "^4.16.4",
    "nock": "^9.0.2",
    "webpack": "^1.13.2",
    "webpack-node-externals": "^1.5.4"
  },
  "dependencies": {
    "es6-promise": "^4.0.5",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.16.4",
    "whatwg-fetch": "^1.0.0"
  }
}
